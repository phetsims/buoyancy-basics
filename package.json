{
  "name": "buoyancy-basics",
  "version": "1.0.0-dev.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/buoyancy-basics.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "BUOYANCY_BASICS",
    "phetLibs": [
      "density-buoyancy-common",
      "mobius"
    ],
    "preload": [
      "../sherpa/lib/poly-decomp-0.3.0.js",
      "../sherpa/lib/p2-0.7.1.js",
      "../sherpa/lib/three-r104.js"
    ],
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phet-io": {},
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsInteractiveDescription": true,
      "supportsDynamicLocale": true
    },
    "screenNameKeys": [
      "BUOYANCY_BASICS/screen.explore",
      "BUOYANCY_BASICS/screen.compare"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/ban-ts-comment": [
            "error",
            {
              "ts-expect-error": true,
              "ts-ignore": true,
              "ts-check": true,
              "ts-nocheck": true
            }
          ]
        }
      }
    ]
  }
}